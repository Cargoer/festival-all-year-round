{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},

  "pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "每逢佳节",
        "app-plus": {
          "titleNView": {
            "backgroundColor": "#F1F6FA",
            "titleAlign": "center"
          }
        }
			}
		},
    {
			"path": "pages/index/addFestival",
			"style": {
				"navigationBarTitleText": "添加节日",
        "app-plus": {
          "titleNView": {
            "backgroundColor": "#F1F6FA",
            "titleAlign": "center"
          }
        }
			}
		},
    {
			"path": "pages/dayCountDown/index",
			"style": {
				"navigationBarTitleText": "节日倒数",
        "app-plus": {
          "titleNView": {
            "backgroundColor": "#F1F6FA",
            "titleAlign": "center"
          }
        }
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
